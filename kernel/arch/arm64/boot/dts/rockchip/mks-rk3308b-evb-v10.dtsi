 &mac {
 	phy-supply = <&vcc_phy>;
 	clock_in_out = "output";
 	assigned-clocks = <&cru SCLK_MAC>;
	pinctrl-0 = <&rmiim1_pins &macm1_refclk_12ma>;
	snps,reset-gpio = <&gpio4 RK_PC0 GPIO_ACTIVE_LOW>;
 	snps,reset-active-low;
 	snps,reset-delays-us = <0 50000 50000>;
 	status = "okay";
 };

