&cpu0 {
	cpu-supply = <&vdd_core>;
};

&cpu0_opp_table {
	opp-1200000000 {
		status = "okay";
	};
	opp-1296000000 {
		status = "okay";
	};
};

&rk3308bs_cpu0_opp_table {
	opp-1008000000 {
		status = "okay";
	};
	opp-1104000000 {
		status = "okay";
	};
};

&dmc {
	center-supply = <&vdd_log>;
	status = "okay";
};

&emmc {
	bus-width = <8>;
	cap-mmc-highspeed;
	// mmc-hs200-1_8v;
	no-sdio;
	no-sd;
	disable-wp;
	non-removable;
	num-slots = <1>;
	status = "okay";
};


&sdmmc {
	max-frequency = <50000000>;
	bus-width = <4>;
	cap-sd-highspeed;
    cap-sdio-irq;
	// no-1-8-v;
	// vmmc-supply = <&vcc_io>;
	// vqmmc-supply = <&vcc_io>;
	// no-sd;
	// no-mmc;
 	supports-sdio;
	rockchip,default-sample-phase = <0x14>;
	keep-power-in-suspend;
	non-removable;
	mmc-pwrseq = <&sdio_pwrseq>;
	// sd-uhs-sdr104;
	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc_bus4 &sdmmc_cmd &sdmmc_clk>;
	status = "okay";
};

&fiq_debugger {
	rockchip,serial-id = <3>;
	status = "okay";
};

&io_domains {
	status = "okay";

	vccio0-supply = <&vcc_io>;
	vccio1-supply = <&vcc_io>;
	vccio2-supply = <&vcc_io>;
	vccio3-supply = <&vccio_flash>;
	vccio4-supply = <&vcc_io>;
	vccio5-supply = <&vcc_io>;
};

